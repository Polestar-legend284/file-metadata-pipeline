{
    "chat.commandCenter.enabled": false,
    "window.menuBarVisibility": "classic",
    "window.commandCenter": true,
    "http.proxySupport": "off",
    "redhat.telemetry.enabled": false,
    "workbench.layoutControl.enabled": false,
    "window.autoDetectColorScheme": true,
    "geminicodeassist.beta.enableCodeDocumentationView": false,
    "geminicodeassist.project": "cloudshell-gca",
    "terminal.integrated.defaultProfile.linux": "Google Cloud Shell",
    "workbench.startupEditor": "none",
    "files.autoSave": "off",
    "editor.fontSize": 6,
    "editor.renderWhitespace": "none",
    "editor.accessibilitySupport": "on",
    "editor.codeActions.triggerOnFocusChange": true,
    "editor.defaultFormatter": "google.cloud-shell",
    "editor.inlineCompletionsAccessibilityVerbose": true,
    "editor.insertSpaces": false,
    "editor.scrollBeyondLastColumn": 5,
    "editor.tabFocusMode": true,
    "typescript.tsserver.watchOptions": "vscode"
}